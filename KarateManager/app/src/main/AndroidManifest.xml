<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.mma.karatemanager">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"><!--
            android:theme="@style/AppTheme.AppBarOverlay"-->
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddCompetitionActivity"
            android:label="@string/Add_compet" />
        <activity android:name=".AddFightActivity"
            android:label="@string/Add_fight" />
        <activity android:name=".CompetitionListActivity"
            android:label="@string/Compet" />
        <activity android:name=".FightActivity"
            android:label="@string/Fight" />
        <activity android:name=".HistoricActivity"
            android:label="@string/Historical" />
    </application>

</manifest>